{
  "expo": {
    "name": "Anime Legends !",
    "slug": "anime-legends",
    "version": "0.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "anime-legends",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "backgroundColor": "#0F0F14",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.animelegends.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F14"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0F0F14",
          "image": "./assets/images/splash-logo.png",
          "dark": {
            "backgroundColor": "#0F0F14",
            "image": "./assets/images/splash-logo.png"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9616ee73-b5fc-4e2f-a89a-18399ba4dfc8"
      }
    }
  }
}